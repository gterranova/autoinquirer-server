{
    "compileOnSave": false,
    "compilerOptions": {
        //"alwaysStrict": true,
        //"strict": true,
        "importHelpers": true,
        "sourceMap": true,
        "declaration": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es6",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2017"
        ],
        "baseUrl": ".",
        "paths": {
            "autoinquirer": [
                "../autoinquirer"
            ]
        },
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules",
        "tmp"
    ]
}