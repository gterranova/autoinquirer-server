{
  "name": "autoinquirer-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --build src/tsconfig.json",
    "watch": "tsc -w -p src/tsconfig.json",
    "clean": "rimraf dist",
    "start": "npm run build && node dist/server.js ../autoinquirer-composer/examples/schema.json ../autoinquirer-composer/examples/data.json",
    "serve": "node --inspect=5858 -r ts-node/register ./src/server.ts ..\\autoinquirer-cli",
    "start:watch": "nodemon"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/socket.io": "^2.1.4",
    "autoinquirer": "file:../autoinquirer",
    "body-parser": "^1.19.0",
    "commander": "^4.0.1",
    "cors": "^2.8.5",
    "delete": "^1.1.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.3",
    "http": "0.0.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/json-schema": "^7.0.4",
    "@types/node": "^12.12.17",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "../autoinquirer/build"
    ],
    "exec": "npm run serve",
    "ext": "ts"
  }
}